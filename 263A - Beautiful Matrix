#include<iostream>
#include<algorithm>
using namespace std;

int main(){
    int arr[5][5];
    int one_x,one_y;
    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
            cin>>arr[i][j];
            if(arr[i][j]==1){
                one_x=i;
                one_y=j;
            }
        }
    }
    cout<<abs(one_x-2)+abs(one_y-2)<<endl;
    return 0;
}
