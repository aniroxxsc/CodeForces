#include<iostream>
#include<unordered_map>

int main(){
    std::string s;
    std::cin>>s;
    std::unordered_map<char,int> um;
    for(int i=0;i<s.size();i++){
        um[s[i]]++;
    }
    if(um.size()%2==0){
        std::cout<<"CHAT WITH HER!"<<std::endl;
    }
    else{
        std::cout<<"IGNORE HIM!"<<std::endl;
    }
    return 0;
}
